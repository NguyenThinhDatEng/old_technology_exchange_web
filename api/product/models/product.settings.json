{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "name": "product",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "maxLength": 100
    },
    "price": {
      "type": "float",
      "required": true,
      "default": 0
    },
    "trademark": {
      "via": "products",
      "model": "trademark"
    },
    "describe": {
      "type": "text",
      "default": "nothing"
    },
    "avatar": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": true,
      "pluginOptions": {}
    },
    "category": {
      "private": true,
      "model": "category",
      "via": "products"
    },
    "other_attributes": {
      "type": "dynamiczone",
      "components": [
        "other-attributes.for-laptop",
        "other-attributes.for-phone",
        "other-attributes.for-tablet",
        "other-attributes.for-smart-watch"
      ]
    },
    "comments": {
      "via": "product",
      "collection": "comment"
    },
    "customer": {
      "model": "customer",
      "via": "products"
    }
  }
}
