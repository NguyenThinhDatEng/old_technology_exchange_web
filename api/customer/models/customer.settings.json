{
  "kind": "collectionType",
  "collectionName": "customers",
  "info": {
    "name": "Customer",
    "description": "This represents the customer Model"
  },
  "options": {
    "increments": true,
    "configurable": false,
    "privateAttributes": [
      "__v",
      "createdAt",
      "updatedAt",
      "id"
    ],
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "username": {
      "type": "string",
      "required": true,
      "unique": true,
      "maxLength": 50
    },
    "email": {
      "type": "email",
      "required": true,
      "unique": true,
      "maxLength": 100
    },
    "phoneNumber": {
      "type": "string",
      "maxLength": 11
    },
    "password": {
      "type": "password",
      "required": true
    },
    "avatar": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "gender": {
      "type": "enumeration",
      "enum": [
        "Male",
        "Female"
      ]
    },
    "notifications": {
      "via": "customer",
      "collection": "notification"
    },
    "carings": {
      "collection": "caring",
      "via": "customer"
    },
    "OTP": {
      "type": "string"
    }
  }
}
